<h1>new wine</h1>
<!--ワイン新規投稿フォーム-->
<%= form_with model: @wine, url: '/wines', local:true do |f| %>
    <h2>name</h2>
    <%= f.text_field :name %>
    <h2>explanation</h2>
    <%= f.text_area :explanation %>
    <H2>image</H2>
    <%= f.attachment_field :image %>
    <h2>genre</h2>
    <%= f.label :genre %>
    <%#= f.select :genre, Genre.wines.keys, :selected=>Genre.wines[@wine.wine] %>
    <%= f.select :genre, Wine.genres.keys.to_a %>
    <%= f.submit 'post' %>
<% end %>